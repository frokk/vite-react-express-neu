{
    "applicationId": "js.neutralino.sample",
    "version": "0.0.0",
    "defaultMode": "window",
    "port": 0,
    "documentRoot": "/www/build/",
    "url": "/",
    "enableServer": true,
    "enableNativeAPI": true,
    "tokenSecurity": "one-time",
    "logging": {
        "enabled": true,
        "writeToLogFile": true
    },
    "nativeAllowList": ["app.*", "os.*"],
    "modes": {
        "window": {
            "title": "Vite React Neutralino",
            "width": 800,
            "height": 500,
            "minWidth": 400,
            "minHeight": 200,
            "fullScreen": false,
            "alwaysOnTop": false,
            "icon": "/www/build/icons/appIcon.png",
            "borderless": false,
            "maximize": false,
            "hidden": false,
            "resizable": true,
            "exitProcessOnClose": true
        },
        "browser": {
            "nativeBlockList": ["filesystem.*"]
        }
    },
    "cli": {
        "binaryName": "vite-react",
        "resourcesPath": "/www/build/",
        "extensionsPath": "/extensions/",
        "clientLibrary": "/www/public/js/neutralino.js",
        "binaryVersion": "4.6.0",
        "clientVersion": "3.5.0",
        "frontendLibrary": {
            "patchFile": "/www/index.html",
            "devUrl": "http://localhost:3000"
        }
    }
}
