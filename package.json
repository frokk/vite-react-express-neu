{
	"name": "express-neu",
	"private": true,
	"version": "0.0.1",
	"scripts": {
		"start": "concurrently -k 'npm:dev:vite' 'npm:dev:neu'",
		"build": "npm run build:vite && npm run build:neu && cpy ./express ./dist/express-neu",
		"dev:neu": "neu run -- --window-enable-inspector",
		"build:neu": "neu build",
		"dev:vite": "vite build --watch",
		"build:vite": "vite build",
		"postinstall": "neu update"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@neutralinojs/neu": "^9.4.0",
		"@vitejs/plugin-react": "^1.0.7",
		"concurrently": "^7.0.0",
		"cpy-cli": "^4.2.0",
		"vite": "^2.8.0"
	}
}
